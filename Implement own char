#include <stdlib.h>

char* my_strdup(const char *src) {
    int len = 0;
    const char *p = src;
    while (*p++) len++;

    char *dup = (char *)malloc((len + 1) * sizeof(char));
    if (!dup) return NULL;

    char *q = dup;
    while (*src)
        *q++ = *src++;
    *q = '\0';

    return dup;
}
